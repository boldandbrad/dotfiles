################################################################
# mac app store installer                                      #
################################################################

brew "mas"

################################################################
# desktop apps                                                 #
################################################################

mas "numbers", id: 409203825        # spreadsheets
mas "parcel", id: 639968404         # package tracker
mas "peek", id: 1554235898          # quicklook enhancer ($)
mas "xcode", id: 497799835          # apple ide

################################################################
# safari extensions                                            #
################################################################

mas "duckduckgo", id: 1482920575    # safari tracker blocker
mas "magic-lasso", id: 1198047227   # safari ad blocker
